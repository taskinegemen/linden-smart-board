<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx" width="155" height="36">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			import Events.PagePaletteClickEvent;
			import mx.controls.Alert;
			import mx.core.FlexGlobals;
			
			protected function backPageClicked(e:MouseEvent): void {
				FlexGlobals.topLevelApplication.dispatchEvent(new PagePaletteClickEvent(PagePaletteClickEvent.LEFT_CLICKED));
			}
			
			
			protected function outLineClicked(e:MouseEvent): void {
				FlexGlobals.topLevelApplication.dispatchEvent(new PagePaletteClickEvent(PagePaletteClickEvent.OUTLINE_CLICKED));
			}
			
			protected function frontPageClicked(e:MouseEvent): void {
				FlexGlobals.topLevelApplication.dispatchEvent(new PagePaletteClickEvent(PagePaletteClickEvent.RIGHT_CLICKED));
			}
			
		]]>
	</fx:Script>
	<s:Image width="76" height="36" source="/Assets/left.png" click="{backPageClicked(event)}">
		
	</s:Image>
	<s:Image x="77" y="0" width="76" height="36" source="/Assets/center.png" click="{outLineClicked(event)}">
		
	</s:Image>
	<s:Image x="154" y="0" width="76" height="36" source="/Assets/right.png" click="{frontPageClicked(event)}">
		
	</s:Image>

	
</s:Group>
